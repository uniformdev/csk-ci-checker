{
  "name": "component-starter-kit",
  "version": "2.0.212",
  "private": true,
  "engines": {
    "npm": ">=8.19.0",
    "node": ">=18.12.0",
    "yarn": "please-use-npm",
    "pnpm": "please-use-npm"
  },
  "scripts": {
    "dev": "run-s pull:manifest dev:next",
    "dev:next": "next dev",
    "build:next": "next build",
    "build": "run-s pull:manifest build:next",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx}\"",
    "pull:manifest": "uniform context manifest download --output ./src/context/manifest.json",
    "uniform:pull:components": "uniform canvas component pull ./content/component-starter-kit.json",
    "uniform:pull:compositions": "uniform canvas composition pull ./content/component-starter-kit.json",
    "uniform:pull:project-map-definitions": "uniform project-map definition pull ./content/component-starter-kit.json",
    "uniform:pull:project-map-nodes": "uniform project-map node pull ./content/component-starter-kit.json",
    "uniform:pull": "run-s uniform:pull:components uniform:pull:compositions uniform:pull:project-map-definitions uniform:pull:project-map-nodes",
    "uniform:push:components": "uniform canvas component push ./content/component-starter-kit.json",
    "uniform:push:compositions": "uniform canvas composition push ./content/component-starter-kit.json",
    "uniform:push:project-map-definitions": "uniform project-map definition push ./content/component-starter-kit.json",
    "uniform:push:project-map-nodes": "uniform project-map node push ./content/component-starter-kit.json",
    "uniform:publish": "uniform canvas composition push ./content/component-starter-kit.json --state published",
    "uniform:push": "run-s uniform:push:components uniform:push:compositions uniform:push:project-map-definitions uniform:push:project-map-nodes",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@uniformdev/canvas-next": "19.10.0",
    "@uniformdev/context-react": "19.10.0",
    "classnames": "^2.3.2",
    "daisyui": "^2.51.5",
    "next": "13.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-multi-carousel": "^2.8.3",
    "sharp": "^0.32.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.7",
    "@storybook/addon-interactions": "^7.0.7",
    "@storybook/addon-links": "^7.0.7",
    "@storybook/addon-styling": "^1.0.3",
    "@storybook/blocks": "^7.0.7",
    "@storybook/nextjs": "^7.0.7",
    "@storybook/react": "^7.0.7",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@types/color": "^3.0.3",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.32",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "@typescript-eslint/types": "^5.59.6",
    "@uniformdev/cli": "19.10.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-storybook": "^0.6.11",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.23",
    "prettier": "^2.8.8",
    "storybook": "^7.0.7",
    "storybook-addon-themes": "^6.1.0",
    "tailwindcss": "^3.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
